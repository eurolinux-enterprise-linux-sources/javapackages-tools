<?xml version="1.0" encoding="US-ASCII"?>
<configuration>
  <resolverSettings>
    <localRepositories>
      <repository>.xm2</repository>
    </localRepositories>
    <jarRepositories>
      <repository>/usr/share/java</repository>
      <repository>/usr/lib/java</repository>
    </jarRepositories>
    <pomRepositories>
      <repository>/usr/share/maven-effective-poms</repository>
      <repository>/usr/share/maven-poms</repository>
      <repository>/usr/share/maven/poms</repository>
      <repository>/usr/share/maven2/poms</repository>
    </pomRepositories>
    <metadataRepositories>
      <repository>/usr/share/maven-fragments</repository>
    </metadataRepositories>
    <prefixes>
      <prefix>/</prefix>
    </prefixes>
    <blacklist>
      <artifact>
        <groupId>javax.activation</groupId>
        <artifactId>activation</artifactId>
      </artifact>
      <artifact>
        <groupId>org.eclipse.jetty.orbit</groupId>
        <artifactId>javax.activation</artifactId>
      </artifact>
      <artifact>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav</artifactId>
      </artifact>
      <artifact>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav-jackrabbit</artifactId>
      </artifact>
    </blacklist>
  </resolverSettings>
  <installerSettings>
    <enableRawPoms>true</enableRawPoms>
    <enableEffectivePoms>true</enableEffectivePoms>
    <pomRawModel>true</pomRawModel>
    <pomEffectiveModel>false</pomEffectiveModel>
    <jarRawModel>true</jarRawModel>
    <jarEffectiveModel>true</jarEffectiveModel>
    <jarDir>usr/share/java</jarDir>
    <jniDir>usr/lib/java</jniDir>
    <pomDir>usr/share/maven-poms</pomDir>
    <effectivePomDir>usr/share/maven-effective-poms</effectivePomDir>
    <metadataDir>usr/share/maven-fragments</metadataDir>
    <installRoot>.xmvn/root</installRoot>
  </installerSettings>
</configuration>
